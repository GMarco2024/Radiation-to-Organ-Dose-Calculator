Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Our", " ", "organs", " ", "of", " ", "interest"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"organs", "=", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{"\"\<Lungs\>\"", ",", "\[IndentingNewLine]", "\"\<Stomach\>\"", 
       ",", "\[IndentingNewLine]", "\"\<Bladder\>\"", ",", 
       "\[IndentingNewLine]", "\"\<Thyroid\>\"", ",", 
       "\[IndentingNewLine]", "\"\<Whole Body\>\""}], "\[IndentingNewLine]", 
      "}"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"In", " ", "units", " ", "of", " ", "cGy"}], ",", " ", 
     RowBox[{"of", " ", "course"}], ",", " ", 
     RowBox[{"we", " ", "have", " ", "our", " ", "absorbed", " ", "doses"}]}],
     "*)"}], "\n", 
   RowBox[{
    RowBox[{"absorbedDoses", "=", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
      "0.13", ",", "\[IndentingNewLine]", "0.24", ",", "\[IndentingNewLine]", 
       "0.15", ",", "\[IndentingNewLine]", "0.7", ",", "\[IndentingNewLine]", 
       "1.9"}], "}"}]}], ";"}], " ", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"We", " ", "have", " ", "our", " ", "weighted", " ", "tissues", " ", 
      RowBox[{"(", "WT", ")"}]}], ",", " ", 
     RowBox[{
     "in", " ", "accordance", " ", "to", " ", "ICRP", " ", "Publicaiton", " ",
       "103"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"tissueWeights", "=", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
      "0.12", ",", "\[IndentingNewLine]", "0.12", ",", "\[IndentingNewLine]", 
       "0.04", ",", "\[IndentingNewLine]", "0.04", ",", "\[IndentingNewLine]",
        "1.0"}], "}"}]}], ";"}], " ", "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Radiation", " ", "types"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"radiationTypes", "=", 
     RowBox[{"{", 
      RowBox[{"\"\<Alpha\>\"", ",", "\"\<Beta\>\"", ",", "\"\<Neutron\>\"", 
       ",", "\"\<Gamma\>\"", ",", "\"\<Gamma\>\""}], "}"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", "Energies", "*)"}], "\n", 
   RowBox[{
    RowBox[{"energies", "=", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
      "0.770", ",", "\[IndentingNewLine]", "1.1", ",", "\[IndentingNewLine]", 
       "1.1", ",", "\[IndentingNewLine]", "0.662", ",", "\[IndentingNewLine]",
        "0.662"}], "\[IndentingNewLine]", "}"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "We", " ", "define", " ", "the", " ", "radiation", " ", "weighting", " ", 
     "factor", " ", "function", " ", "based", " ", "on", " ", "ICRP", " ", 
     "103"}], "*)"}], "\[IndentingNewLine]", "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"wrFunction", "[", 
      RowBox[{"type_", ",", "energy_"}], "]"}], ":=", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Switch", "[", 
      RowBox[{
      "type", ",", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", "\"\<Alpha\>\"", ",", "20", ",", 
       "\[IndentingNewLine]", "\"\<Beta\>\"", ",", "1", ",", 
       "\[IndentingNewLine]", "\"\<Gamma\>\"", ",", "1", ",", 
       "\[IndentingNewLine]", "\"\<Neutron\>\"", ",", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"NumericQ", "[", "energy", "]"}], ",", "\[IndentingNewLine]",
          "\[IndentingNewLine]", 
         RowBox[{"Which", "[", 
          RowBox[{
           RowBox[{"energy", "<", "1"}], ",", 
           RowBox[{"2.5", "+", 
            RowBox[{"18.2", " ", 
             RowBox[{"Exp", "[", 
              RowBox[{
               RowBox[{"-", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"Log", "[", "energy", "]"}], ")"}], "^", "2"}]}], "/",
                "6"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", 
           RowBox[{"1", "<=", "energy", "<=", "50"}], ",", 
           RowBox[{"5", "+", 
            RowBox[{"17", " ", 
             RowBox[{"Exp", "[", 
              RowBox[{
               RowBox[{"-", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"Log", "[", 
                   RowBox[{"2", " ", "energy"}], "]"}], ")"}], "^", "2"}]}], "/",
                "6"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", 
           RowBox[{"energy", ">", "50"}], ",", 
           RowBox[{"2.5", "+", 
            RowBox[{"3.25", " ", 
             RowBox[{"Exp", "[", 
              RowBox[{
               RowBox[{"-", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"Log", "[", 
                   RowBox[{"0.04", " ", "energy"}], "]"}], ")"}], "^", 
                 "2"}]}], "/", "6"}], "]"}]}]}]}], "]"}], ",", "Null"}], " ", 
        "]"}]}], "]"}]}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Compute", " ", "WR", " ", "for", " ", "each", " ", "exposure"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"wrValues", "=", 
     RowBox[{"MapThread", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"wrFunction", "[", 
         RowBox[{"#1", ",", "#2"}], "]"}], "&"}], ",", 
       RowBox[{"{", 
        RowBox[{"radiationTypes", ",", "energies"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{"\"\<\\nThe wrValues: \>\"", ",", " ", "wrValues"}], "]"}], 
    ";"}], "\n", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Compute", " ", "Equivalent", " ", "Doses", " ", "HT", " ", "in", " ", 
     "mSv"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"HT_mSv", "=", 
     RowBox[{"absorbedDose_cGy", "*", "WR", "*", "10"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"equivalentDoses", "=", 
     RowBox[{"absorbedDoses", "*", "wrValues", "*", "10"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{"\"\<\\nEquivalentDoses: \>\"", ",", " ", "equivalentDoses"}], 
     "]"}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Compute", " ", "Effective", " ", "Doses", " ", "E", " ", "in", " ", 
      "mSv"}], ",", " ", 
     RowBox[{
      RowBox[{"in", " ", "which", " ", "E"}], " ", "=", " ", 
      RowBox[{
       RowBox[{"(", "WT", ")"}], 
       RowBox[{"(", "HT", ")"}]}]}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"effectiveDoses", "=", 
     RowBox[{"tissueWeights", "*", "equivalentDoses"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<\\nEffective Dose\>\"", ",", " ", "effectiveDoses"}], "]"}], 
   "\n", 
   InterpretationBox[
    RowBox[{"\"\<\\nEffective Dose\>\"", "\[InvisibleSpace]", 
     RowBox[{"{", 
      RowBox[{
      "3.12`", ",", "0.288`", ",", "1.2196076129743134`", ",", "0.28`", ",", 
